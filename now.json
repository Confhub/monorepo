{
  "name": "confhub",
  "alias": "confhub.win",
  "public": false,
  "type": "docker",
  "features": {
    "cloud": "v2"
  },
  "scale": {
    "bru1": {
      "min": 0,
      "max": "auto"
    },
    "sfo1": {
      "min": 0,
      "max": "auto"
    }
  },
  "build": {
    "env": {
      "MAPBOX_SECRET": "@confhub-mapbox-secret",
      "GRAPHQL_ENDPOINT_URL": "@confhub-graphql-endpoint-url"
    }
  }
}
